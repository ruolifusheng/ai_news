{
  "version": "1.0",
  "ai": {
    "provider": "doubao",
    "model": "doubao-seed-2-0-pro-260215",
    "base_url": "https://ark.cn-beijing.volces.com/api/v3",
    "api_key_env": "DOUBAO_API_KEY",
    "temperature": 0.3,
    "max_tokens": 8192,
    "languages": ["zh", "en"]
  },
  "sources": {
    "github": [
      {
        "type": "user_events",
        "username": "karpathy",
        "enabled": true
      },
      {
        "type": "repo_releases",
        "owner": "vllm-project",
        "repo": "vllm",
        "enabled": true
      },
      {
        "type": "repo_releases",
        "owner": "sgl-project",
        "repo": "sglang",
        "enabled": true
      },
      {
        "type": "repo_releases",
        "owner": "triton-lang",
        "repo": "triton",
        "enabled": true
      }
    ],
    "hackernews": {
      "enabled": true,
      "fetch_top_stories": 10,
      "min_score": 200
    },
    "rss": [
      {
        "name": "Simon Willison",
        "url": "https://simonwillison.net/atom/everything/",
        "enabled": true,
        "category": "ai-tools"
      },
      {
        "name": "GitHub Trending - Daily",
        "url": "https://mshibanami.github.io/GitHubTrendingRSS/daily/all.xml",
        "enabled": true,
        "category": "github-trending"
      },
      {
        "name": "LWN.net",
        "url": "https://lwn.net/headlines/full_text?key=${LWN_KEY}",
        "enabled": true,
        "category": "linux-kernel"
      }
    ],
    "reddit": {
      "enabled": true,
      "subreddits": [
        {
          "subreddit": "MachineLearning",
          "enabled": true,
          "sort": "hot",
          "time_filter": "day",
          "fetch_limit": 15,
          "min_score": 60
        },
        {
          "subreddit": "LocalLLaMA",
          "enabled": true,
          "sort": "hot",
          "time_filter": "day",
          "fetch_limit": 15,
          "min_score": 60
        }
      ],
      "users": [],
      "fetch_comments": 10
    },
    "telegram": {
      "enabled": true,
      "channels": [
        {
          "channel": "zaihuapd",
          "enabled": true,
          "fetch_limit": 20
        }
      ]
    }
  },
  "filtering": {
    "ai_score_threshold": 7.0,
    "time_window_hours": 24
  }
}
